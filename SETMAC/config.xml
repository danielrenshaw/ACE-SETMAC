  <docTypes>
    <setmac:flagged
      name="QuestionOnly"
      labelPropertyKey="State"
      includeAnswers="false"
      includeQuestionComments="false"
      includeAnswerComments="false" />
    <setmac:flagged
      name="QuestionAndAnswers"
      labelPropertyKey="State"
      includeAnswers="true"
      includeQuestionComments="false"
      includeAnswerComments="false" />
    <setmac:flagged
      name="QuestionAndAnswersAndComments"
      labelPropertyKey="State"
      includeAnswers="true"
      includeQuestionComments="true"
      includeAnswerComments="true" />
  </docTypes>
  
  <classifiers>
    <classifier
      name="NaiveBayes"
      type="uk.ac.ed.inf.setmac.classifiers.NaiveBayes" />
    <setmac:lda name="LDA" />
    <setmac:dmr name="DMR" />
    <setmac:simpleLda name="SimpleLDA" enabled="false" />
    <setmac:ldaSvm name="LdaSvm" />
  </classifiers>
  
  <tasks>
    <binaryIntegerTask
      name="OffTopic"
      labelPropertyKey="State"
      trueLabel="1" />
    <binaryIntegerTask
      name="NotConstructive"
      labelPropertyKey="State"
      trueLabel="2" />
    <binaryIntegerTask
      name="NotARealQuestion"
      labelPropertyKey="State"
      trueLabel="3" />
    <binaryIntegerTask
      name="TooLocalized"
      labelPropertyKey="State"
      trueLabel="4" />
    <multiClassIntegerTask
      name="MultiClass"
      labelPropertyKey="State"
      labels="0 1 2 3 4" />
  </tasks>
  
  <preCacheProcessors>
    <processor
      name="ExtractHtmlContent"
      code="EHC"
      type="uk.ac.ed.inf.ace.processors.ExtractHtmlContent" />
    <tokenizer
      name="Tokenizer"
      code="TOK"
      pattern="[^\w']" />
    <processor
      name="CleanTokens"
      code="CTK"
      type="uk.ac.ed.inf.ace.processors.CleanTokens" />
    <processor
      name="TokensToLower"
      code="TTL"
      type="uk.ac.ed.inf.ace.processors.TokensToLower" />
    <removeStopwords
      name="RemoveStopwords"
      code="RSW"
      pathname="..\mallet-2.0.7\stoplists\en.txt" />
    <processor
      name="CompressByDictionary"
      code="TTL"
      type="uk.ac.ed.inf.ace.processors.CompressByDictionary" />
  </preCacheProcessors>
  
  <postCacheProcessors>
    <filterByLabel name="FilterByLabel" code="FBL" labelPropertyKey="State">
      <rule label="0" sampleProbability="0.1" />
    </filterByLabel>
  </postCacheProcessors>
  
  <programmes>
    <programme name="Baseline" enabled="false">
      <dataSets>
        <dataSetRef name="Programmers" />
      </dataSets>
      <docTypes>
        <docTypeRef name="QuestionOnly" />
      </docTypes>
      <classifiers>
        <classifier
          name="Randomifier"
          type="uk.ac.ed.inf.setmac.classifiers.Randomifier" />
      </classifiers>
      <postCacheProcessors />
    </programme>
    <programme name="FixedLDA">
      <dataSets>
        <dataSetRef name="Programmers" />
      </dataSets>
      <docTypes>
        <docTypeRef name="QuestionOnly" />
      </docTypes>
      <classifiers>
        <setmac:lda name="LDA-LTR-MD" instanceModelDistanceType="LeftToRight" classifierType="MinimumDistance" />
        <setmac:lda name="LDA-LTR-GD" instanceModelDistanceType="LeftToRight" classifierType="GaussianDistance" enabled="false" />
        <setmac:lda name="LDA-MT-MD" instanceModelDistanceType="MeanTopics" classifierType="MinimumDistance" enabled="false" />
        <setmac:lda name="LDA-MT-GD" instanceModelDistanceType="MeanTopics" classifierType="GaussianDistance" enabled="false" />
      </classifiers>
    </programme>
    <programme name="NaiveBayes" enabled="false">
      <dataSets>
        <dataSetRef name="Programmers" />
      </dataSets>
      <docTypes>
