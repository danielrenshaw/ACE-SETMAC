    <programme name="LDA" enabled="false">
      <dataSets>
        <dataSetRef name="Programmers" />
      </dataSets>
      <docTypes>
        <docTypeRef name="QuestionOnly" />
      </docTypes>
      <classifiers>
        <setmac:lda name="LDA" />
        <setmac:lda name="LDA-5t" numTopics="5" />
        <setmac:lda name="LDA-20t" numTopics="20" />
        <setmac:lda name="LDA-100t" numTopics="100" />
        <setmac:lda name="LDA-200t" numTopics="200" />
        <setmac:lda name="LDA-400t" numTopics="400" />
